{
  "productName": "SpoolSync",
  "version": "0.1.0",
  "identifier": "dev.gaelicthunder.spoolsync",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "SpoolSync Desktop",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "withGlobalTauri": true
  },
  "bundle": {
    "active": false,
    "targets": "all"
  },
  "plugins": {}
}
